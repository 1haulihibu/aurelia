{
  "name": "@benchmarking-apps/runner",
  "version": "1.0.0",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.5",
    "mocha": "^8.1.3",
    "playwright": "^1.6.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -b -w --preserveWatchOutput",
    "::e2e": "mocha --ui bdd --colors --reporter spec --timeout 1200000",
    "bench": "npm run ::e2e -- dist/bench.spec.js"
  }
}
